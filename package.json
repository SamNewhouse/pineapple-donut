{
  "name": "pineapple-donut",
  "version": "1.0.0",
  "description": "",
  "main": ".build/handler.js",
  "scripts": {
    "start": "./start.sh",
    "stop": "./stop.sh",
    "build": "tsc",
    "deploy": "serverless deploy",
    "offline": "serverless offline --stage dev",
    "dynamo:up": "docker compose up -d",
    "dynamo:down": "docker compose down",
    "dynamo:destroy": "docker compose down -v",
    "dynamo:reset": "npm run dynamo:destroy && npm run dynamo:up",
    "table:create": "ts-node src/scripts/create-table.ts",
    "table:seed": "ts-node src/scripts/seed.ts",
    "clean": "rm -rf .build",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.910.0",
    "@aws-sdk/util-dynamodb": "3.910.0",
    "axios": "1.12.2",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "9.0.2",
    "random-words": "2.0.1",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.155",
    "@types/dotenv": "^6.1.1",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.7.2",
    "prettier": "3.6.2",
    "serverless": "4.20.2",
    "serverless-offline": "14.4.0",
    "typescript": "5.9.3"
  }
}
